    "sourceMap": true,
    "outDir": "./dist/",
    "strict": true,
    "baseUrl": "./src/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
    },
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "node_modules"
    ]
}
